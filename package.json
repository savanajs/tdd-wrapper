{
  "name": "spotify",
  "version": "1.0.0",
  "description": "Nossa biblioteca do curso de JS com TDD na pr√°tica.",
  "main": "src/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build_cmd": "./node_modules/.bin/babel --out-dir lib src",
    "build": "npm run clear && npm run build_cmd",
    "build:watch": "npm run build -- --watch",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.min.js -p",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "babel": "babel-node exemple/albums.js"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willianjusten/spotify.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Willian Justen <willianjustenqui@gmail.com> (https://willianjusten.com.br/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willianjusten/spotify/issues"
  },
  "homepage": "https://github.com/willianjusten/spotify#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "0.11.9",
    "mocha": "^5.1.1",
    "node-fetch": "^2.1.2",
    "nyc": "^11.8.0",
    "rimraf": "^2.6.3",
    "sinon": "2.1.0",
    "sinon-chai": "^3.0.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^2.4.1",
    "babel-loader": "^7.0.0"
  },
  "dependencies": {
    "node-fetch": "^2.3.0"
  }
}
